{
  "name": "glace",
  "type": "module",
  "displayName": "Glace",
  "description": "A glace theme",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Glace",
        "uiTheme": "vs-dark",
        "path": "./themes/generated.json"
      }
    ]
  },
  "scripts": {
    "build": "ts-node src",
    "watch": "nodemon --watch src --exec 'pnpm build'",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^20.5.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
