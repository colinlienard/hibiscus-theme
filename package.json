{
  "name": "glace",
  "displayName": "Glace",
  "version": "0.0.1",
  "description": "A glace theme",
  "categories": [
    "Themes"
  ],
  "type": "module",
  "scripts": {
    "build": "tsx src",
    "format": "prettier --write .",
    "watch": "tsx watch src"
  },
  "contributes": {
    "themes": [
      {
        "label": "Glace",
        "uiTheme": "vs-dark",
        "path": "./themes/generated.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^20.5.2",
    "prettier": "^3.0.2",
    "tsx": "^3.12.7"
  },
  "engines": {
    "vscode": "^1.81.0"
  }
}
