{
  "name": "hibiscus-theme",
  "displayName": "Hibiscus Theme",
  "version": "0.3.0",
  "description": "A warm, cosy, pastel VSCode theme",
  "icon": "assets/icon.png",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark",
    "warn",
    "cosy",
    "pastel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/colinlienard/hibiscus-theme"
  },
  "publisher": "colinlienard",
  "scripts": {
    "dev": "bun src/index.ts",
    "format": "prettier --write .",
    "watch": "bun --watch run src/index.ts"
  },
  "contributes": {
    "themes": [
      {
        "label": "Hibiscus",
        "uiTheme": "vs-dark",
        "path": "./themes/generated.json"
      }
    ]
  },
  "devDependencies": {
    "bun-types": "latest",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2",
    "vsce": "^2.15.0"
  },
  "engines": {
    "vscode": "^1.81.0"
  }
}
